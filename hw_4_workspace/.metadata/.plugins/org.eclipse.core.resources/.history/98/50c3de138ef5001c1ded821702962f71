	@Test
	public void testClassReflection() {
		Class<Comando> cl = Comando.class;
		Class<ComandoAiuto> ca = ComandoAiuto.class;
		for(Class c: cl.getClasses()) {
			System.out.println(c.getName());
		}
		
		for(Class c: ca.getClasses()) {
			System.out.println(c.getName());
		}
		System.out.println(cl.getName());
	}
